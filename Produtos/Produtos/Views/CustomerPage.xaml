<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="Produtos.Views.CustomerPage"
             Title="Nome do Programa"
             BackgroundColor="{StaticResource BackgroundColor}">
    <ContentPage.ToolbarItems>
        <ToolbarItem Icon="filtro.png" Command="{Binding GoToCommand}" CommandParameter="NewOrderPage">
        </ToolbarItem>
    </ContentPage.ToolbarItems>
    <StackLayout Padding="40" HorizontalOptions="Center">
        <Label Text="Pagina Clientes" TextColor="{StaticResource FontColor}"></Label>
    </StackLayout>
  
</ContentPage>